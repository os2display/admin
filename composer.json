{
  "name": "os2display/admin",
  "license": "GNU General Public License v3.0",
  "type": "project",
  "description": "The Os2Display distribution",
  "autoload": {
    "psr-0": {
      "": "src/",
      "SymfonyStandard": "app/"
    }
  },
  "require": {
    "php": ">=5.3.3",
    "symfony/symfony": "2.8.*",
    "sensio/distribution-bundle": "~3.0",
    "sensio/framework-extra-bundle": "~3.0",
    "incenteev/composer-parameter-handler": "~2.0",
    "doctrine/orm": "~2.2,>=2.2.3",
    "doctrine/doctrine-bundle": "~1.2",
    "twig/extensions": "~1.0",
    "symfony/assetic-bundle": "~2.3",
    "symfony/swiftmailer-bundle": "~2.3",
    "symfony/monolog-bundle": "~2.4",
    "friendsofsymfony/user-bundle": "~1.3",
    "sonata-project/core-bundle": "~2",
    "sonata-project/doctrine-orm-admin-bundle": "~2",
    "sonata-project/admin-bundle": "~2",
    "sonata-project/doctrine-extensions": "~1.0",
    "sonata-project/media-bundle": "~2",
    "debril/rss-atom-bundle": "2.2",
    "jms/serializer-bundle": "~0.13",
    "jms/di-extra-bundle": "1.4.*@dev",
    "jms/job-queue-bundle": "dev-master",
    "pagerfanta/pagerfanta": "^1.0",
    "guzzlehttp/guzzle": "^6.2.1",
    "doctrine/doctrine-migrations-bundle": "^1.0",
    "nelmio/api-doc-bundle": "^2.13",
    "itk/os2display-koba-integration": "^1.0",
    "itk/os2display-aarhus-templates": "^1.0",
    "os2display/core-bundle": "^1.0",
    "os2display/admin-bundle": "^1.0",
    "os2display/media-bundle": "^1.0",
    "os2display/default-template-bundle": "^1.0"
  },
  "require-dev": {
    "sensio/generator-bundle": "~2.3",
    "behat/mink-extension": "^2.2",
    "behat/symfony2-extension": "^2.1",
    "behat/mink-selenium2-driver": "^1.3",
    "behat/mink-browserkit-driver": "^1.3",
    "behatch/contexts": "^2.6"
  },
  "repositories": [
    {
      "type": "vcs",
      "url":  "https://github.com/itk-os2display/core-bundle"
    },
    {
      "type": "vcs",
      "url":  "https://github.com/itk-os2display/admin-bundle"
    },
    {
      "type": "vcs",
      "url":  "https://github.com/itk-os2display/media-bundle"
    },
    {
      "type": "vcs",
      "url":  "https://github.com/itk-os2display/default-template-bundle"
    },
    {
      "type": "vcs",
      "url":  "https://github.com/itk-os2display/os2display-koba-integration"
    },
    {
      "type": "vcs",
      "url":  "https://github.com/itk-os2display/os2display-aarhus-templates"
    }
  ],
  "scripts": {
    "post-root-package-install": [
      "SymfonyStandard\\Composer::hookRootPackageInstall"
    ],
    "post-install-cmd": [
      "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::removeSymfonyStandardFiles"
    ],
    "post-update-cmd": [
      "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
      "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::removeSymfonyStandardFiles"
    ]
  },
  "config": {
    "bin-dir": "bin"
  },
  "extra": {
    "symfony-app-dir": "app",
    "symfony-web-dir": "web",
    "symfony-assets-install": "symlink",
    "incenteev-parameters": {
      "file": "app/config/parameters.yml"
    },
    "branch-alias": {
      "dev-master": "2.5-dev"
    }
  }
}
